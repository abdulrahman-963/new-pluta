{
  "info": {
    "name": "Pluta Camera AI API",
    "_postman_id": "pluta-camera-ai-api",
    "description": "API collection for Pluta Camera AI - Video processing, stream management, and AI object detection",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{jwt_token}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8081/api",
      "type": "string"
    },
    {
      "key": "jwt_token",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Video Management",
      "item": [
        {
          "name": "Upload Video",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": []
                },
                {
                  "key": "zoneId",
                  "value": "1",
                  "type": "text"
                },
                {
                  "key": "cameraId",
                  "value": "1",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/v1/video/upload",
              "host": ["{{base_url}}"],
              "path": ["v1", "video", "upload"]
            },
            "description": "Upload a video file for AI-powered processing and analysis"
          }
        },
        {
          "name": "Get All Videos",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/v1/video?page=0&size=20&sort=id,desc",
              "host": ["{{base_url}}"],
              "path": ["v1", "video"],
              "query": [
                {
                  "key": "page",
                  "value": "0"
                },
                {
                  "key": "size",
                  "value": "20"
                },
                {
                  "key": "sort",
                  "value": "id,desc"
                }
              ]
            },
            "description": "Retrieve a paginated list of all videos"
          }
        },
        {
          "name": "Get Videos by Tenant and Branch",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/v1/video/by-tenant-branch?page=0&size=20",
              "host": ["{{base_url}}"],
              "path": ["v1", "video", "by-tenant-branch"],
              "query": [
                {
                  "key": "page",
                  "value": "0"
                },
                {
                  "key": "size",
                  "value": "20"
                }
              ]
            },
            "description": "Get videos filtered by current user's tenant and branch"
          }
        },
        {
          "name": "Get Video by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/v1/video/:videoId",
              "host": ["{{base_url}}"],
              "path": ["v1", "video", ":videoId"],
              "variable": [
                {
                  "key": "videoId",
                  "value": "1"
                }
              ]
            },
            "description": "Get detailed information about a specific video"
          }
        },
        {
          "name": "Get Video Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/v1/video/:videoId/status",
              "host": ["{{base_url}}"],
              "path": ["v1", "video", ":videoId", "status"],
              "variable": [
                {
                  "key": "videoId",
                  "value": "1"
                }
              ]
            },
            "description": "Check the processing status of a video"
          }
        }
      ]
